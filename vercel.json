{
  "version": 2,
  "functions": {
    "sriharidevelopers/wsgi.py": {
      "runtime": "@vercel/python"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sriharidevelopers/wsgi.py"
    }
  ],
  "env": {
    "SECRET_KEY": "django-insecure-vercel-production-key-abc123xyz789",
    "DEBUG": "False",
    "ALLOWED_HOSTS": ".vercel.app"
  }
}